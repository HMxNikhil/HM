# Example environment variables for AI Hackathon Hub
# Copy this file to .env and fill values before running the server

# MongoDB connection URI (MongoDB Atlas or local)
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net
MONGODB_DB=ai_hackathon_hub

# External AI provider API key (optional)
EMERGENT_API_KEY=

# Frontend origin for CORS (adjust if different)
CLIENT_ORIGIN=http://localhost:5173

# Optional preview origin used in CORS
PREVIEW_ORIGIN=https://89526568e31742afa21709ebb3bdb718-7a7142784232428b99c71dd48.projects.builder.codes

# Server port (default 4000)
PORT=4000
